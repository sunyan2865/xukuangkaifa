<div class="wrapper" id="wrapper">
    <!-- 头部区 -->
    <div class="seeyon-portal-header" id="seeyonPortalHeader">
        <!-- 左侧：logo、切换兼职单位 -->
        <div class="seeyonElementGroup logoNameConcurrent">
            <!-- logo -->
            <div class="seeyonElement" id="logoAndName" data-tpl="tpl-wideLogo"></div>
            <!-- 切换兼职单位 -->
            <div class="seeyonElement" id="concurrentAccount" data-tpl="tpl-concurrentAccount"></div>
        </div>
        <div class="seeyonElementGroup spaceNavOperationBtn height-inherit lineHeight-inherit">
            <!-- 右侧操作区 -->
            <div class="seeyonElement" id="systemOperationBtn" data-tpl="tpl-topRightsystemOperation"></div>
            <!-- 空间导航 -->
            <div class="seeyonElement height-inherit lineHeight-inherit navElement" id="spaceNav" data-tpl="tpl-topCenterNav"></div>
        </div>
    </div>
    <!-- 我的快捷 + 菜单 -->
    <div class="seeyonElementGroup shortcutAndMenuNav menuHotspotsArea clearfix">
        <!-- 头像、在线状态、在线人数 -->
        <div class="seeyonElementGroup account4in1 ">
            <div class="accountNav" id="accountNav">
                <div class="headLeft">
                    <div class="seeyonElement" id="personHeadImg" data-tpl="tpl-largeHead"></div>
                    <div class="seeyonElement" id="onlineStatus" data-tpl="tpl-onlineStatus"></div>
                </div>
                <div class="headRight">
                    <div class="seeyonElement" id="onlineNumArea" data-tpl="tpl-onlineNum"></div>
                </div>
            </div>
        </div>
        <!-- 我的快捷 -->
        <div class="seeyonElement" id="shortcutNav" data-tpl="tpl-topShortcutNav"></div>
        <!-- 菜单 -->
        <div class="seeyonElement left menuElement" id="menuNav" data-tpl="tpl-topMenuNav"></div>
    </div>
    <!-- 中间区 -->
    <div class="seeyon-portal-body" id="seeyonPortalBody">
        <!-- 面包屑 -->
        <div class="breadcrumb" id="breadcrumb"></div>
        <!-- 栏目区 -->
        <div class="main" id="main"></div>
    </div>
    <!-- 消息盒子(底部弹出) -->
    <div class="seeyonElement" id="messageBox" data-tpl="tpl-messageBottom"></div>
    <!-- 首页设置 -->
    <div class="seeyonElement" id="topSkinSet" data-tpl="tpl-skinSet"></div>
</div>